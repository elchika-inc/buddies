{
  "name": "@pawmatch/crawler",
  "version": "1.0.0",
  "description": "PawMatch data crawler for pet adoption websites",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "crawl:dogs": "tsx src/scripts/crawl-all.ts pet-home --animal-type dogs --transform",
    "crawl:cats": "tsx src/scripts/crawl-all.ts pet-home --animal-type cats --transform",
    "crawl:pets": "tsx src/scripts/crawl-all.ts pet-home --animal-type dogs --transform",
    "crawl:all": "tsx src/scripts/crawl-all.ts all --transform",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "playwright": "^1.40.0",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "date-fns": "^2.30.0",
    "fs-extra": "^11.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.9.0",
    "tsx": "^4.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "crawler",
    "pets",
    "adoption",
    "scraping"
  ],
  "author": "PawMatch Team",
  "license": "MIT"
}
{
  "extends": "../tsconfig.workers.json",
  "compilerOptions": {
    "types": ["@cloudflare/workers-types"],
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "lib": ["ESNext", "WebWorker", "DOM"],
    "paths": {
      "@/*": ["./src/*"],
      "@shared/*": ["../shared/src/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
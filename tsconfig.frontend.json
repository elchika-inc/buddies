{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "ES6"],
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "incremental": true,
    "allowJs": true,
    "types": ["node"],
    "noUncheckedSideEffectImports": true
  }
}
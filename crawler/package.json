{
  "name": "@pawmatch/crawler",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --local",
    "build": "wrangler build",
    "deploy": "wrangler deploy --env production",
    "tail": "wrangler tail",
    "test": "../../scripts/crawler/dev/test-crawlers.sh",
    "db:init": "node ../../scripts/crawler/dev/init-db.js",
    "db:reset": "../scripts/dev/reset-dev-db.sh",
    "seed:api": "node ../../scripts/crawler/dev/seed-api-data.js",
    "seed:sample": "node ../../scripts/crawler/dev/create-sample-data.js",
    "crawl:dogs": "npm run seed:api -- --type=dog",
    "crawl:cats": "npm run seed:api -- --type=cat",
    "crawl:pethome": "../../scripts/crawler/crawl-pethome.sh",
    "crawl:pethome:dogs": "../../scripts/crawler/crawl-pethome.sh --type dog",
    "crawl:pethome:cats": "../../scripts/crawler/crawl-pethome.sh --type cat",
    "crawl:pethome:all": "../../scripts/crawler/crawl-pethome.sh --type both --limit 20",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,json}'"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.872.0",
    "cheerio": "^1.1.2",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.4",
    "linkedom": "^0.18.12",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@eslint/js": "^9.35.0",
    "@types/dom-parser": "^0.1.4",
    "@types/node": "^20.16.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.20.5",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.33.0"
  }
}

    "postcss": "^8.4.27",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "prettier": "^3.6.2"
  },
  "engines": {
    "bun": ">=1.0.0",

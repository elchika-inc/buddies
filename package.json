{
  "name": "pawmatch",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "保護犬・保護猫とユーザーをマッチングするWebアプリケーション",
  "keywords": [
    "react",
    "typescript",
    "graphql",
    "apollo-client",
    "pet-adoption",
    "matching"
  ],
  "scripts": {
    "dev": "vite",
    "dev:pages": "wrangler pages dev dist --local --d1 DB=pawmatch-db",
    "build": "vite build",
    "build:check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "crawl": "tsx src/scripts/crawl.ts",
    "crawl:dogs": "tsx src/scripts/crawl.ts dogs",
    "crawl:cats": "tsx src/scripts/crawl.ts cats",
    "crawl:status": "tsx src/scripts/crawl.ts status",
    "crawl:d1": "tsx src/scripts/crawl-d1.ts",
    "crawl:d1:dogs": "tsx src/scripts/crawl-d1.ts dogs",
    "crawl:d1:cats": "tsx src/scripts/crawl-d1.ts cats",
    "crawl:d1:status": "tsx src/scripts/crawl-d1.ts status"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@graphql-tools/schema": "^10.0.25",
    "@tanstack/react-router": "^1.58.3",
    "clsx": "^2.0.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@tanstack/react-router-devtools": "^1.58.3",
    "@tanstack/router-plugin": "^1.58.4",
    "@types/node": "^24.2.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.2",
    "vite": "^7.0.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}

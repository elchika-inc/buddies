# PawMatch Frontend Environment Variables Example
# Copy this file to .env.local and update the values

# Pet type configuration (dog or cat)
NEXT_PUBLIC_PET_TYPE=cat

# API Configuration
# Local development API URL
NEXT_PUBLIC_API_URL=http://localhost:8787
# Production API URL (example)
# NEXT_PUBLIC_API_URL=https://api.pawmatch.app

# Enable API data fetching (true for development)
NEXT_PUBLIC_USE_API=true

# Use sample data instead of real database (true for development)
NEXT_PUBLIC_USE_SAMPLE_DATA=true

# App Configuration
NEXT_PUBLIC_APP_NAME=CatMatch
NEXT_PUBLIC_APP_URL=http://localhost:3004
